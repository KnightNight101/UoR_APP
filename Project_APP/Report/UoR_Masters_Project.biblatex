@article{meyerWorkLifeDevelopers2017,
  title = {The {{Work Life}} of {{Developers}}: {{Activities}}, {{Switches}} and {{Perceived Productivity}}},
  shorttitle = {The {{Work Life}} of {{Developers}}},
  author = {Meyer, André N. and Barton, Laura E. and Murphy, Gail C. and Zimmermann, Thomas and Fritz, Thomas},
  date = {2017-12},
