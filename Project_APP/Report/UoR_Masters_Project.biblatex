@inproceedings{carneiroScrumAgileProject2018,
  title = {Scrum {{Agile Project Management Methodology Application}} for {{Workflow Management}}: {{A Case Study}}},
  shorttitle = {Scrum {{Agile Project Management Methodology Application}} for {{Workflow Management}}},
  booktitle = {2018 {{IEEE International Conference}} on {{Industrial Engineering}} and {{Engineering Management}} ({{IEEM}})},
  author = {Carneiro, L. B. and Silva, A. C. C. L. M. and Alencar, L. H.},
